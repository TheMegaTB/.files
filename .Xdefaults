! urxvt
!urxvt*font:		xft:Hack-Regular
urxvt*font:         xft:SauceCodePro NF:regular:pixelsize=15
urxvt*foreground:       #cccccc
urxvt*scrollBar:        false
urxvt*perl-lib:         /usr/lib/urxvt/perl/
urxvt*perl-ext-common:  default,matcher
urxvt*urlLauncher:      /usr/bin/jumanji
urxvt*matcher.button:   1

Xft*dpi:                96
Xft*antialias:          true
Xft*hinting:            full

urxvt*background:       rgba:0000/0000/0000/cccc

URxvt.transparent:   true
URxvt.tintColor:     white
URxvt.shading:       50 

URxvt*color0:      #000000
URxvt*color1:      #9e1828
URxvt*color2:      #aece92
URxvt*color3:      #968a38
URxvt*color4:      #414171
URxvt*color5:      #963c59
URxvt*color6:      #418179
URxvt*color7:      #bebebe
URxvt*color8:      #666666
URxvt*color9:      #cf6171
URxvt*color10:     #c5f779
URxvt*color11:     #fff796
URxvt*color12:     #4186be
URxvt*color13:     #cf9ebe
URxvt*color14:     #71bebe
URxvt*color15:     #ffffff

! rofi
rofi.font: Hack 15
rofi.width: 100
rofi.lines: 5
rofi.columns: 1
rofi.opacity: 80
! row color, font color, 2nd row color, selected row, selected font
rofi.color-normal: #000000,#505050,#000000,#000000,#FFB964
rofi.color-urgent: #000000,#505050,#000000,#000000,#FFB964
rofi.color-active: #000000,#505050,#000000,#000000,#FFB964
rofi.color-window: #000000,#002B36
rofi.bw: 0
rofi.location: 2
rofi.padding: 100
